<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#d3d3d3</item>
        <item name="colorPrimaryDark">#d3d3d3</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="generalnotitle" parent="AppTheme">
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="ProperCheck">
        <item name="colorAccent">#2196F3</item>
    </style>

    <style name="page_background_wh">
        <item name="android:background">#fafafa</item>
    </style>

    <style name="text_dark_gray">
        <item name="android:textColor">#a9a9a9</item>
    </style>

    <style name="text_black">
        <item name="android:textColor">#353535</item>
    </style>

    <style name="nav_background_light_gray">
        <item name="android:background">#d3d3d3</item>
    </style>

    <style name="text_wh">
        <item name="android:textColor">#fafafa</item>
    </style>

    <style name="page_background_off_black">
        <item name="android:background">#353535</item>
    </style>

    <style name="nav_background_bl">
        <item name="android:background">#24282a</item>
    </style>

    <style name="nav_list_wh">
        <item name="android:background">#fafafa</item>
        <item name="android:textColor">#353535</item>
    </style>

    <style name="nav_list_black">
        <item name="android:background">#353535</item>
        <item name="android:textColor">#fafafa</item>
    </style>

    <style name="btn_background_wh">
        <item name="android:color">#fafafa</item>
    </style>

    <style name="btn_background_black">
        <item name="android:color">#353535</item>
    </style>

    <style name="edit_text_border_black">
        <item name="android:color">#353535</item>
    </style>

    <style name="edit_text_border_wh">
        <item name="android:color">#fafafa</item>
    </style>

</resources>
